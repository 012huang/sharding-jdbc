<?xml version="1.0" encoding="UTF-8"?>
<asserts>
    <assert id="assertInsertWithAllPlaceholders" sql="INSERT INTO t_order (order_id, user_id, status) VALUES (%s, %s, %s)" parameters="1,1,'insert'">
        <tables>
            <table name="t_order" />
        </tables>
        <conditions>
            <condition column-name="order_id" table-name="t_order" operator="EQUAL">
                <value index="0"/>
            </condition>
            <condition column-name="user_id" table-name="t_order" operator="EQUAL">
                <value index="1"/>
            </condition>
        </conditions>
    </assert>
